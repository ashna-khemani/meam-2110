<?xml version="1.0" encoding="utf-8"?>
<!-- =================================================================================== -->
<!-- |    This document is a hand-crafted representation of a singular floating base   | -->
<!-- |    of cube shape for MEAM 2110.                                                 | -->
<!-- =================================================================================== -->
<robot name="floating_cube">
  <link name="inertial_frame">
   <!-- Need an empty object to make a joint with the one link -->
    <inertial>
      <mass value="0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>


  <link name="body_F">
    <inertial>
      <!-- mass=0.5 -->
      <mass value="0.5"/>
      
      <!-- r_Co_Ccm=[0, 0, 0], R_C_V = eye(3) -->
      <origin xyz="0 0 0" rpy="0 0 0"/>

      <!-- I_C_Ccm = zeros(3) -->
      <inertia ixx="0.0052" ixy="0" ixz="0" iyy="0.0052" iyz="0" izz="0.0052"/>
    </inertial>

    <visual>
      <!-- r_Co_Vo=[0, 0, 0], R_C_V = eye(3) -->
      <origin xyz="0 0 0" rpy="0 0 0"/>

      <geometry>
        <!-- sides=[0.25 0.25 0.25] -->
        <box size="0.25 0.25 0.25"/>
      </geometry>
    </visual>
  </link>
<!-- =================================================================================== -->

  <link name="body_F1">
    <inertial>
      <!-- mass=0.5 -->
      <mass value="0.5"/>
      
      <!-- r_Co_Ccm=[0, 0, 0], R_C_V = eye(3) -->
      <origin xyz="0 0 0" rpy="0 0 0"/>

      <!-- I_C_Ccm = zeros(3) -->
      <inertia ixx="0.0052" ixy="0" ixz="0" iyy="0.0052" iyz="0" izz="0.0052"/>
    </inertial>

    <visual>
      <!-- r_Co_Vo=[0, 0, 0], R_C_V = eye(3) -->
      <origin xyz="0 0 0" rpy="0 0 0"/>

      <geometry>
        <!-- sides=[0.25 0.25 0.25] -->
        <box size="0.25 0.25 0.25"/>
      </geometry>
    </visual>
  </link>

 

<!-- =================================================================================== -->

  <joint name="trans_x" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit upper="1000" lower="-1000" effort="10000" velocity="10000"/>
    <parent link="inertial_frame" />
    <child link="body_F" />
    <axis xyz="1 0 0"/>
  </joint>

<!-- =================================================================================== -->

  <joint name="trans_x1" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit upper="1000" lower="-1000" effort="10000" velocity="10000"/>
    <parent link="body_F" />
    <child link="body_F1" />
    <axis xyz="1 0 0"/>
  </joint>

<!-- =================================================================================== -->

  <!-- <joint name="trans_x2" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit upper="1000" lower="-1000" effort="10000" velocity="10000"/>
    <parent link="body_F1" />
    <child link="body_F2" />
    <axis xyz="1 0 0"/>
  </joint> -->

</robot>
