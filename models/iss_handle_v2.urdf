<?xml version="1.0" encoding="utf-8"?>
<!-- =================================================================================== -->
<!-- |    This document is a hand-crafted representation of the MEAM 2110 ISS          | -->
<!-- |    handle, with mass at the last link.                                          | -->
<!-- =================================================================================== -->
<robot name="iss_handle">
  <link name="inertial_frame">
   <!-- Need an empty object to make a joint with the one link -->
    <inertial>
      <mass value="0"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>
  </link>

  <link name="frame_A">
  <!-- Single character link names aren't properly parsed by ParseVisual.m -->
   <!-- Need an empty object to get to 6-DOF joint to racket handle (F) -->
  </link>

  <link name="frame_B">
   <!-- Need an empty object to get to 6-DOF joint to racket handle (F) -->
  </link>

  <link name="frame_C">
   <!-- Need an empty object to get to 6-DOF joint to racket handle (F) -->
  </link>

  <link name="frame_D">
   <!-- Need an empty object to get to 6-DOF joint to racket handle (F) -->
  </link>

  <link name="frame_E">
   <!-- Need an empty object to get to 6-DOF joint to racket handle (F) -->
  </link>

  <link name="body_F">
    <inertial>
      <!-- mass=0 -->
      <mass value="0"/>
      
      <!-- r_Co_Ccm=[0, 0, 0], R_C_V = eye(3) -->
      <origin xyz="0 0 0" rpy="0 0 0"/>

      <!-- I_C_Ccm = zeros(3) -->
      <inertia ixx="0" ixy="0" ixz="0" iyy="0" iyz="0" izz="0"/>
    </inertial>

    <visual>
      <!-- r_Co_Vo=[0, 0, 0], R_C_V = eye(3) -->
      <origin xyz="0 0 0" rpy="0 0 0"/>

      <geometry>
        <!-- sides=[0.2 0.2 2] -->
        <box size="0.2 0.2 2"/>
      </geometry>
    </visual>
  </link>

  <link name="body_G">
    <inertial>
      <!-- mass=3.5 -->
      <mass value="3.5"/>
      
      <!-- r_Co_Ccm=[0, 0, 0], R_C_V = eye(3) -->
      <origin xyz="0 0 0" rpy="0 0 0"/>

      <!-- I_C_Ccm = diag(16.7653, 5.2453, 12.52) -->
      <inertia ixx="16.7653" ixy="0" ixz="0" iyy="5.2453" iyz="0" izz="12.52"/>
    </inertial>

    <visual>
      <!-- r_Co_Vo=[0, 0, 0.1429], R_C_V = eye(3) -->
      <origin xyz="0 0 0.1429" rpy="0 0 0"/>

      <geometry>
        <!-- sides=[0.4 2 0.4] -->
        <box size="0.4 2 0.4"/>
      </geometry>
    </visual>
  </link>


  <joint name="trans_x" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit upper="1000" lower="-1000" effort="10000" velocity="10000"/>
    <parent link="inertial_frame" />
    <child link="frame_A" />
    <axis xyz="1 0 0"/>
  </joint>
  <joint name="trans_y" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit upper="1000" lower="-1000" effort="10000" velocity="10000"/>
    <parent link="frame_A" />
    <child link="frame_B" />
    <axis xyz="0 1 0"/>
  </joint>
  <joint name="trans_z" type="prismatic">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <limit upper="1000" lower="-1000" effort="10000" velocity="10000"/>
    <parent link="frame_B" />
    <child link="frame_C" />
    <axis xyz="0 0 1"/>
  </joint>
  <joint name="rot_x" type="continuous">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="frame_C" />
    <child link="frame_D" />
    <axis xyz="1 0 0"/>
  </joint>
  <joint name="rot_y" type="continuous">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="frame_D" />
    <child link="frame_E" />
      <axis xyz="0 1 0"/>
  </joint>
  <joint name="rot_z" type="continuous">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="frame_E" />
    <child link="body_F" />
      <axis xyz="0 0 1"/>
  </joint>
  <joint name="joint_weld" type="fixed">
    <!-- r_Po_Jo=[0,0,0.8571], R_P_J=eye(3) -->
    <origin xyz="0 0 0.8571" rpy="0 0 0"/>

    <parent link="body_F"/>
    <child link="body_G"/>
  </joint>
</robot>
